<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest ApplicationTypeName="CircuitBreakerType"
                     ApplicationTypeVersion="1.0.0"
                     xmlns="http://schemas.microsoft.com/2011/01/fabric"
                     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Parameters>
    <Parameter Name="CounterService_MinReplicaSetSize" DefaultValue="3" />
    <Parameter Name="CounterService_PartitionCount" DefaultValue="1" />
    <Parameter Name="CounterService_TargetReplicaSetSize" DefaultValue="3" />
    <Parameter Name="WeatherService_MinReplicaSetSize" DefaultValue="3" />
    <Parameter Name="WeatherService_PartitionCount" DefaultValue="1" />
    <Parameter Name="WeatherService_TargetReplicaSetSize" DefaultValue="3" />
    <Parameter Name="CounterService_InstanceCount" DefaultValue="-1" />
    <Parameter Name="WeatherService_InstanceCount" DefaultValue="-1" />
    <Parameter Name="Web1_InstanceCount" DefaultValue="-1" />
  </Parameters>
  <!-- Import the ServiceManifest from the ServicePackage. The ServiceManifestName and ServiceManifestVersion 
       should match the Name and Version attributes of the ServiceManifest element defined in the 
       ServiceManifest.xml file. -->
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="CounterServicePkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="WeatherServicePkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="CounterServicePkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="WeatherServicePkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="Web1Pkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <DefaultServices>
    <Service Name="CounterService">
      <StatefulService ServiceTypeName="CounterServiceType" TargetReplicaSetSize="[CounterService_TargetReplicaSetSize]" MinReplicaSetSize="[CounterService_MinReplicaSetSize]">
        <UniformInt64Partition PartitionCount="[CounterService_PartitionCount]" LowKey="-9223372036854775808" HighKey="9223372036854775807" />
      </StatefulService>
    </Service>
    <Service Name="WeatherService">
      <StatefulService ServiceTypeName="WeatherServiceType" TargetReplicaSetSize="[WeatherService_TargetReplicaSetSize]" MinReplicaSetSize="[WeatherService_MinReplicaSetSize]">
        <UniformInt64Partition PartitionCount="[WeatherService_PartitionCount]" LowKey="-9223372036854775808" HighKey="9223372036854775807" />
      </StatefulService>
    </Service>
    <Service Name="Web1">
      <StatelessService ServiceTypeName="Web1Type" InstanceCount="[Web1_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
  </DefaultServices>
</ApplicationManifest>